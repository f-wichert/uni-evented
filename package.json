        "@react-navigation/bottom-tabs": "^6.4.1",
        "@react-navigation/elements": "^1.3.12",
        "@react-navigation/native": "^6.0.16",
        "@react-navigation/native-stack": "^6.9.2",
        "dayjs": "^1.11.7",
