{
    "name": "evented",
    "version": "1.0.0",
    "scripts": {
        "prepare": "husky install",
        "start": "expo start",
        "go:android": "expo start --android",
        "go:web": "expo start --web",
        "native:start": "expo start --dev-client --port 19000",
        "native:install": "expo run:android --port 19000",
        "apk:development": "eas build --platform android --local --profile development --output evented-dev.apk",
        "apk:production": "eas build --platform android --local --profile production --output evented.apk",
        "tsc": "tsc",
        "lint": "eslint .",
        "format": "prettier . --write",
        "format:check": "prettier . --check"
    },
    "dependencies": {
        "@expo/react-native-action-sheet": "^4.0.1",
