        "lint-staged": "^13.0.4",
        "prettier": "^2.8.0",
        "prettier-plugin-organize-imports": "^3.2.1",
        "typescript": "^4.9.3"
    },
    "private": true,
    "lint-staged": {
        "**/*.{js,ts,jsx,tsx,json}": [
            "npm run format"
        ]
    }
